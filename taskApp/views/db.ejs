<!DOCTYPE html>
<html lang="ja">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <%- include('header', title) %>
    <p id="msg"><%= message %></p>
    <table class="table">
        <thead>
            <th>id</th>
            <th>createdAt</th>
            <th>name</th>
            <th>password</th>
        </thead>
        <tbody>
            <% data.forEach(element => { %>
             <tr>
                <td><%= element.id %></td>
                <td><%= element.createdAt %></td>
                <td><%= element.name %></td>
                <td><%= element.password %></td>
             </tr>
            <% }) %>
        </tbody>
    </table>
    <%- include('footer') %>
</body>
</html>
